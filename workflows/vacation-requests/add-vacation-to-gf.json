      "parameters": {
        "jsCode": "// Find member by email\n//const inputData = $('Prepare Vacation Data').first().json;\n//const email = inputData.member_email; \nconst email = $('Called by Agent').first().json.member_email;\nconst members = $input.all();\n\nif (!email) {\n  throw new Error('Email not provided.');\n}\n\n// Find member with matching email\nconst member = members.find(m => m.json.email === email || m.json.email?.toLowerCase() === email.toLowerCase());\n\nif (!member) {\n  throw new Error(`Member with email ${email} not found`);\n}\n\nreturn {\n  json: {\n    memberId: member.json.id,\n    memberName: member.json.name,\n    memberEmail: member.json.email,\n    officeId: member.json.office_id\n  }\n};"
      },
