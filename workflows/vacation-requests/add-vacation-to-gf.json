      "parameters": {
        "jsCode": "const vacations = $input.all();\n//const inputData = $('Prepare Vacation Data').first().json;\n//const targetVacationId = $('Prepare Vacation Data').first().json.vacation_id;\nconst targetVacationId = $('Called by Agent').first().json.vacation_id\n\nif (!targetVacationId) {\n  throw new Error('Vacation ID not provided.');\n}\n\n// Find vacation with matching ID\nconst vacation = vacations.find(v => v.json.id == targetVacationId);\n\nif (vacation) {\n  return {\n    json: {\n      found: true,\n      vacationId: vacation.json.id,\n      vacation: vacation.json\n    }\n  };\n} else {\n  return {\n    json: {\n      found: false,\n      vacationId: targetVacationId\n    }\n  };\n}"
      },
