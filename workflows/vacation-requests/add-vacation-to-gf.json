    {
      "parameters": {
        "jsCode": "const headers = $input.item.json.headers || {};\nlet setCookieArray = headers['set-cookie'] || headers['Set-Cookie'];\n\nif (!setCookieArray) {\n  const headerKeys = Object.keys(headers);\n  const cookieKey = headerKeys.find(k => k.toLowerCase() === 'set-cookie');\n  if (cookieKey) {\n    setCookieArray = headers[cookieKey];\n  }\n}\n\nif (!setCookieArray || !Array.isArray(setCookieArray)) {\n  throw new Error('set-cookie header not found or not an array');\n}\n\n// Find the _glass-factory_session cookie\nconst sessionCookie = setCookieArray.find(cookie => cookie.includes('_glass-factory_session='));\n\nif (!sessionCookie) {\n  throw new Error('_glass-factory_session cookie not found in set-cookie array');\n}\n\n// Extract just the cookie name and value (before first semicolon)\nconst cookieValue = sessionCookie.split(';')[0];\n\nreturn {\n  json: {\n    sessionCookie: cookieValue\n  }\n};"
      },
